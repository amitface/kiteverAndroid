<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_micro_site_editor"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="sms19.listview.newproject.fragment.MicroSiteEditor">

    <ProgressBar
        android:id="@+id/progressBarMicroSiteEditor"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_below="@+id/separator_below"
        android:layout_gravity="center"
        android:indeterminate="true" />
    <WebView
        android:id="@+id/activityMicroSiteEditor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
